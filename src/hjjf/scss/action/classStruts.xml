<!-- START SNIPPET: xworkSample -->
<struts>

	<!-- Some or all of these can be flipped to true for debugging -->
	<constant name="struts.enable.DynamicMethodInvocation" value="true" />
	<constant name="struts.devMode" value="true" />
	
	<package name="ClassAction" extends="employee" namespace="/cls">
	
		<action name="add" class="hjjf.scss.action.ClassAction" method="add">
			<result name="success">/WEB-INF/content/admin/class.jsp</result>
			<result name="error">/WEB-INF/content/admin/class.jsp</result>
		</action>
		
		<action name="delete" class="hjjf.scss.action.ClassAction" method="delete">
			<result name="success">/WEB-INF/content/admin/class.jsp</result>
			<result name="error">/WEB-INF/content/admin/class.jsp</result>
		</action>
		
		<action name="update" class="hjjf.scss.action.ClassAction" method="update">
			<result name="success">/WEB-INF/content/admin/class.jsp</result>
			<result name="error">/WEB-INF/content/admin/class.jsp</result>
		</action>
		
		<action name="search" class="hjjf.scss.action.ClassAction" method="search">
			
		</action>
	</package>
	
</struts>

<!-- END SNIPPET: xworkSample -->

