<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping >
	<class name="hjjf.scss.model.Admin" table="T_Admin">
		<id name="AdminID" column="AdminID" type="string" length="50">
			<generator class="assigned"></generator>
		</id>
		<property name="AdminName" column="AdminName" type="string" length="50"></property>
		<property name="PassWord" column="Password" type="string" length="50"></property>
		<property name="Role" column="Role" type="string" length="50"></property>
		<property name="Photo" column="Photo" type="string" length="50"></property>
		<property name="Remark" column="Remark" type="text" ></property>
	</class>
</hibernate-mapping>
